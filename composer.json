{
    "name": "5818959/smstraffic-notification-channel",
    "description": "SmsTraffic Notification Channel for laravel.",
    "keywords": ["laravel", "notifications", "sms", "smstraffic"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey S.",
            "email": "5818959@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/notifications": "~5.8.0|^6.0",
        "5818959/smstraffic-client": "^0.4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminate\\Tests\\Notifications\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminate\\Notifications\\SmsTrafficChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
